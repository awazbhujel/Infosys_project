      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Using WHERE Clause: Filtering rows before grouping\n",
        "print(\"Using WHERE Clause:\")\n",
        "cursor.execute('''\n",
        "SELECT product, category, amount\n",
        "FROM sales\n",
        "WHERE amount > 500\n",
        "''')\n",
        "for row in cursor.fetchall():\n",
        "    print(row)"
